#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string s;
	    cin>>s;
	    int counter=0,flag=1;
	    for(int i=0;i<n;i++){
	        if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'){
	            counter=0;
	        }else{
	            counter++;
	        }
	        if(counter>=4){
	            flag=0;
	            break;
	        }
	    }
	    if(flag==0){
	        cout<<"NO"<<endl;
	    }else{
	        cout<<"YES"<<endl;
	    }
	}
}
